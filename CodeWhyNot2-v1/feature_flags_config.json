      "rollout_percentage": 50.0,
      "target_environments": [
        "development",
        "staging"
      ],
      "metadata": {
        "category": "counterfactuals",
        "priority": "medium"
      }
    },
    {
      "name": "enable_interactive_visualization",
      "description": "Enable interactive visualization features",
      "enabled": true,
      "rollout_percentage": 100.0,
      "target_environments": [
        "development",
        "staging",
        "production"
      ],
      "metadata": {
        "category": "ui",
        "priority": "high"
      }
    },
    {
      "name": "enable_ml_counterfactuals",
      "description": "Enable ML-based counterfactual generation",
      "enabled": false,
      "rollout_percentage": 0.0,
      "target_environments": [
        "development"
      ],
      "metadata": {
