{
  "settings": {
    "default_provider": "local",
    "cache_ttl_seconds": 300,
    "enable_metrics": true,
    "enable_logging": true,
    "environment": "development"
  },
  "flags": [
    {
      "name": "enable_enhanced_fidelity",
      "description": "Enable enhanced fidelity scoring with AST-level analysis",
